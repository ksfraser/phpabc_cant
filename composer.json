  "authors": [
    {
      "name": "ksfraser"
    }
  ],
  "repositories": [],
  "autoload": {
    "psr-4": {
  "Ksfraser\\PhpabcCanntaireachd\\": "src/Ksfraser/PhpabcCanntaireachd/",
  "Ksfraser\\PhpabcCanntaireachd\\Voices\\": "src/Ksfraser/PhpabcCanntaireachd/Voices/",
      "Ksfraser\\PhpabcCanntaireachd\\Tune\\": "src/Ksfraser/PhpabcCanntaireachd/Tune/",
